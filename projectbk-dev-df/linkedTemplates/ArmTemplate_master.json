{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "projectbk-dev-df"
		},
		"AzureBlobStorage_dev_LS_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "Dev-SA-Access-Secret"
		},
		"AzureKeyVault_Dev_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://projectbkdevKV.vault.azure.net/"
		},
		"triggerevent_properties_Copy Data_parameters_filenamepipe": {
			"type": "string",
			"defaultValue": "@triggerbody().filename"
		},
		"triggerevent_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/00cd90bc-db5d-49ea-85b3-a7995f02d6f8/resourceGroups/projectbk_dev_rg/providers/Microsoft.Storage/storageAccounts/projectbkdevsa"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "projectbk-dev-df_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureKeyVault_Dev_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault_Dev_properties_typeProperties_baseUrl')]"
					},
					"AzureBlobStorage_dev_LS_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('AzureBlobStorage_dev_LS_properties_typeProperties_connectionString_secretName')]"
					},
					"triggerevent_properties_Copy Data_parameters_filenamepipe": {
						"value": "[parameters('triggerevent_properties_Copy Data_parameters_filenamepipe')]"
					},
					"triggerevent_properties_typeProperties_scope": {
						"value": "[parameters('triggerevent_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}